{
  "name": "nextjs-app-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "fix": "npm run fix:eslint && npm run fix:prettier",
    "fix:eslint": "eslint ./src/components --ext .js --ext .jsx --fix",
    "fix:prettier": "prettier --write \"**/*.{js,jsx}\"",
    "lint": "npm run lint:eslint && npm run lint:prettier && npm run lint:styles",
    "lint:eslint": "eslint ./src/components --ext .js --ext .jsx",
    "lint:prettier": "prettier --check \"**/*.{js,jsx}\"",
    "lint:styles": "stylelint ./src/components/**/*.jsx",
    "check:packages": "npx npm-check-updates",
    "packages:upgrade": "npx npm-check-updates -u"
  },
  "dependencies": {
    "@apollo/client": "^3.3.11",
    "axios": "^0.21.1",
    "babel-eslint": "^10.1.0",
    "bootstrap": "^4.6.0",
    "graphql": "^15.5.0",
    "lodash": "^4.17.21",
    "next": "10.0.8",
    "next-redux-saga": "^4.1.2",
    "next-redux-wrapper": "^6.0.2",
    "prop-types": "^15.7.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-redux": "^7.2.2",
    "react-star-ratings": "^2.3.0",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "sass": "^1.32.8"
  },
  "devDependencies": {
    "eslint": "^7.21.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^5.1.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "redux-devtools-extension": "^2.13.9",
    "stylelint": "^13.12.0",
    "stylelint-config-recommended": "^4.0.0",
    "stylelint-custom-processor-loader": "^0.6.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint"
    ]
  }
}
